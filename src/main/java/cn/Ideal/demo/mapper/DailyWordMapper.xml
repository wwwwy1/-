<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.Ideal.demo.mapper.DailyWordMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.Ideal.demo.entity.DailyWord">
        <id column="id" property="id" />
        <result column="content" property="content" />
        <result column="is_deleted" property="isDeleted" />
        <result column="insert_date" property="insertDate" />
        <result column="update_date" property="updateDate" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, content, is_deleted, insert_date, update_date
    </sql>

</mapper>
