<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.Ideal.demo.mapper.StatisticMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.Ideal.demo.entity.Statistic">
        <id column="id" property="id" />
        <result column="user_id" property="userId" />
        <result column="tags_id" property="tagsId" />
        <result column="count" property="count" />
        <result column="insert_date" property="insertDate" />
        <result column="update_date" property="updateDate" />
        <result column="is_deleted" property="isDeleted" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, user_id, tags_id, count, insert_date, update_date, is_deleted
    </sql>

</mapper>
